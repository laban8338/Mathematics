void main() {
  // Define two variables
  int a = 5;
  int b = 3;

  // Addition
  int sum = a + b;
  print("Sum: $sum");

  // Subtraction
  int difference = a - b;
  print("Difference: $difference");

  // Multiplication
  int product = a * b;
  print("Product: $product");

  // Division
  double quotient = a / b;
  print("Quotient: $quotient");

  // Modulo (Remainder)
  int remainder = a % b;
  print("Remainder: $remainder");
}
